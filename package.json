{
  "name": "social-gaming",
  "version": "1.0.0",
  "description": "Typescript Node.js API for gaming social media",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfvmarques/social-gaming.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api",
    "game"
  ],
  "author": "Henrique Marques",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hfvmarques/social-gaming/issues"
  },
  "homepage": "https://github.com/hfvmarques/social-gaming#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.31",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
}
